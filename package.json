		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@digitak/tsc-esm": "^3.1.4",
		"@tanstack/react-query": "^4.26.1",
		"@types/node": "^18.15.1",
		"@types/yargs": "^17.0.22",
